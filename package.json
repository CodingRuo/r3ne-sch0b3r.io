    "keywords": [
        "portfolio",
        "interactive-cv",
        "developer-portfolio",
        "terminal",
        "vanilla-js",
        "codingruo",
        "rene-schober"
    ],
    "packageManager": "pnpm@10.10.0",
    "devDependencies": {
        "@changesets/cli": "^2.29.5",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3"
    }
